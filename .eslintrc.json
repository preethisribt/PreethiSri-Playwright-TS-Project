{
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint"],
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "plugin:playwright/recommended"
    ],
    "rules": {
        "quotes": ["error", "single"]
    },
    "overrides": [
        {
            "files": ["*/playwright-report/", "**/*.{js,jsx}"]
        }
    ]
}
